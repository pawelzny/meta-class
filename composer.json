{
    "name": "pawelzny/meta-class",
    "description": "Framework agnostic MetaClass support for PHP classes",
    "keywords": [
        "meta",
        "MetaClass"
    ],
    "type": "library",
    "license": "ISC",
    "authors": [
        {
            "name": "Paweł Zadrożny",
            "email": "pawel.zny@gmail.com",
            "homepage": "https://pawelzny.com",
            "role": "Owner and Developer"
        }
    ],
    "support": {
        "email": "pawel.zny@gmail.com"
    },
    "require": {
        "php" : "^5.6 || ^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0",
        "friendsofphp/php-cs-fixer": "^2.0",
        "phpmetrics/phpmetrics": "^2.0"
    },
    "scripts": {
        "test": "phpunit",
        "fixer": "php-cs-fixer fix --config=./.php_cs"
    },
    "autoload": {
        "files": [
            "src/Support/mutation.php",
            "src/Support/normalization.php",
            "src/Support/predication.php"
        ],
        "psr-4": {
            "Pawelzny\\": "src/",
            "Pawelzny\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable"
}
